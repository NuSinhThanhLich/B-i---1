#include <bits/stdc++.h>
using namespace std;
#define faster() ios_base::sync_with_stdio(0);cin.tie(NULL);cout.tie(NULL);
const int mod = 1e9 + 7;

int main () {
	int t;
	cin >> t;
	while (t--) {
		int n,k;
	cin >> n >> k;
	int a[n + 5];
	for (int i = 0; i < n; i++) {
		cin >> a[i];
	}
	sort(a , a + n);
	for (int i = n - 1; i >= n - k; i--) {
		cout << a[i] << " ";
	}
	cout << endl;
		
	}
}
