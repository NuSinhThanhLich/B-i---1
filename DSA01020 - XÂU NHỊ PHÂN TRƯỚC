#include <bits/stdc++.h>
using namespace std;
#define faster() ios_base::sync_with_stdio(0);cin.tie(NULL);cout.tie(NULL);
const int mod = 1e9 + 7;

void pre (string a) {
	int i = a.size() - 1;
	while (i >= 0 && a[i] == '0') {
		a[i] = '1';
		i--;
	}
	if (i != -1) {
		a[i] = '0';
	}
	cout << a << endl;
}

int main () {
	int t;
	cin >> t;
	while (t--) {
		string s;
		cin >> s;
		pre(s);
	}
}
