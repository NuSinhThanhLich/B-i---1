#include <bits/stdc++.h>
using namespace std;
#define faster() ios_base::sync_with_stdio(0);cin.tie(NULL);cout.tie(NULL);
const long long mod = 1e9 + 7;


int main () {
	int t;
	cin >> t;
	while (t--) {
		int n,m;
		cin >> n >> m;
		vector<int> v;
		long long a[n + 5];
		long long b[m + 5];
		for (int i = 0; i < n; i++) {
			cin >> a[i];
			v.push_back(a[i]);
		}
		for (int i = 0; i < m; i++) {
			cin >> b[i];
			v.push_back(b[i]);
		}
		sort(v.begin(), v.end());
		for (auto x : v) {
			cout << x << " ";
		}
		cout << endl;
	}
}
