#include <bits/stdc++.h>
#define endl "\n"
using namespace std;

int main () {
	int t;
	cin >> t;
	while (t--) {
		int n;
		cin >> n;
		int a[n + 5];
		stack<int> st;
		for (int i = 0; i < n; i++) {
			cin >> a[i];
		}
		for (int i = 0; i < n; i++) {
			while (!st.empty() && a[i] >= a[st.top()]) {
				st.pop();
			}
			if (st.empty()) {
				cout << i + 1 << " ";
			} else {
				cout << i - st.top() << " ";
			}
			st.push(i);
		}
		cout << endl;
	}
}
