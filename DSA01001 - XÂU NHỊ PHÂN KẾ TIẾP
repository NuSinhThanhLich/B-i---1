#include <bits/stdc++.h>
using namespace std;
#define faster() ios_base::sync_with_stdio(0);cin.tie(NULL);cout.tie(NULL);
const int mod = 1e9 + 7;

void next (string s) {
	int i = s.size() - 1;
	while (i >= 0 && s[i] == '1') {
		s[i] = '0';
		i--;
	}
	if (i != -1) {
		s[i] = '1';
	}
	cout << s << endl;
}

int main () {
	int t;
	cin >> t;
	while (t--) {
		string s;
		cin >> s;
		next(s);
	}
}
