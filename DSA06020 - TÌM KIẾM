#include <bits/stdc++.h>
using namespace std;
const long long mod = 1e9 + 7;

int main () {
	int t;
	cin >> t;
	while (t--) {
		int n,k;
		cin >> n >> k;
		int a[100000];
		int check = 0;
		for (int i = 0; i < n; i++) {
			cin >> a[i];
			if (a[i] == k) {
				check = 1;
			}
		}
		if (check == 1) {
			cout << "1" << endl;
		} else {
			cout << "-1" << endl;
		}
	}
}
